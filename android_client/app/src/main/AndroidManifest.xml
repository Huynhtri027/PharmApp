<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jakub.pharmapp" >
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ShoppingCart"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShoppingResult"
            android:label="@string/title_activity_shopping_result" >

            <!-- android:parentActivityName=".ShoppingCart"> -->

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jakub.pharmapp.ShoppingCart" />
        </activity>
        <activity
            android:name=".look4drug"
            android:label="@string/title_activity_look4drug" >
        </activity>
        <activity
            android:name=".Statistics"
            android:label="@string/title_activity_statistics" >
        </activity>
    </application>

</manifest>
